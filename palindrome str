class Solution {
    int isPalindrome(String s) {
        // code here
        String str = "";
        for(int i=s.length()-1;i>=0;i--){
            str += s.charAt(i);        
        }
        for(int i=0;i<s.length();i++){
            if(str.charAt(i) != s.charAt(i)){
                return 0;
            }
        }
        return 1;
        
    }
};
