class Solution {
    String removeDups(String s) {
        // code here
        HashSet<Character> set = new HashSet<>();
        
        StringBuilder sb = new StringBuilder(); 
        
        for(char c : s.toCharArray()){
            if(!set.contains(c)){
                sb.append(c);
                set.add(c);
            }
        }
        return sb.toString();
    }
}
