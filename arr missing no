class Solution {
    int missingNumber(int arr[], int n) {
        // Your Code Here
        int sum = 0;
        int sum1 = 0;
        
        for(int i=0;i<=n;i++){
            sum += i;
        }
        for(int i=0;i<arr.length;i++){
            sum1  += arr[i];
        }
        return Math.abs(sum - sum1);
    }
}
