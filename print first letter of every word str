class Solution {
    String firstAlphabet(String S) {
        // code here
        String s = "";
        s += S.charAt(0);
        for(int i=0;i<S.length();i++){
            if(S.charAt(i) == ' '){
                if(S.charAt(i+1) != ' '){
                    s += S.charAt(i+1);
                }    
            }
        }
        return s;
    }
};
